-- Doctors who gave out prescriptions of Vicodin --
CREATE VIEW VicoDoctors AS 
SELECT p.FirstName, p.LastName 
FROM person p, doctor d 
WHERE d.DoctorID= (SELECT pv.DoctorID
		        FROM patientVisit pv
		       WHERE pv.VisitID IN (SELECT v.VisitID
		       			FROM PVisitPrescription v, prescription s
					      WHERE v.PrescriptionID = s.PrescriptionID 
                AND s.PrescriptionName = "Vicodin"));
